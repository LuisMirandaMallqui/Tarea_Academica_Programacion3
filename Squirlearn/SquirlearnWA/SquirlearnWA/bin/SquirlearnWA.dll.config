<system.serviceModel>
        <bindings>
                <basicHttpBinding>
                        <binding name="CategoriaPortBinding" />
                        <binding name="ColorPortBinding" />
                        <binding name="CondicionPortBinding" />
                        <binding name="FormatoPortBinding" />
                        <binding name="PersonaPortBinding" />
                        <binding name="PublicacionPortBinding" />
                        <binding name="SubcategoriaPortBinding" />
                        <binding name="TamanoPortBinding" />
                </basicHttpBinding>
        </bindings>
        <client>
                <endpoint address="http://localhost:8080/SquirlearnWS/Categoria"
                        binding="basicHttpBinding" bindingConfiguration="CategoriaPortBinding"
                        contract="categoriaSOAP.Categoria" name="CategoriaPort" />
                <endpoint address="http://localhost:8080/SquirlearnWS/Color"
                        binding="basicHttpBinding" bindingConfiguration="ColorPortBinding"
                        contract="colorSOAP.Color" name="ColorPort" />
