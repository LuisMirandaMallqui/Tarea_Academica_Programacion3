                        contract="formatoSOAP.Formato" name="FormatoPort" />
                <endpoint address="http://localhost:8080/SquirlearnWS/Persona"
                        binding="basicHttpBinding" bindingConfiguration="PersonaPortBinding"
                        contract="personaSOAP.Persona" name="PersonaPort" />
                <endpoint address="http://localhost:8080/SquirlearnWS/Publicacion"
                        binding="basicHttpBinding" bindingConfiguration="PublicacionPortBinding"
                        contract="publicacionSOAP.Publicacion" name="PublicacionPort" />
                <endpoint address="http://localhost:8080/SquirlearnWS/Subcategoria"
